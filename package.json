    "start:ts-node": "ts-node --compiler ttypescript ./src/index.tsx",
    "start:babel": "babel-node -r ./register.c ./src/index.tsx",
    "start:watch": "nodemon --config nodemon.json",
    "start:webpack": "webpack --config webpack.config.ts --mode development --watch",
    "build:webpack": "webpack --config webpack.config.ts --mode production",
    "start:production": "yarn run build:webpack && node --inspect=5858 -r ts-node/register ./src/index.tsx"
